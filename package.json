{
  "name": "pixlcrypt",
  "version": "1.0.0",
  "description": "",
  "main": "lib/app.js",
  "private": true,
  "scripts": {
    "aws:install:cli": "pip install awscli --upgrade --user",
    "aws:ecs:auth": "eval $( aws ecr get-login --no-include-email --region eu-west-1 )",
    "aws:ecs:deploy": "npm run aws:ecs:auth && npm run docker:build && npm run docker:tag && npm run docker:push",
    "prebuild": "rm -rf build ; mkdir -p build",
    "build": "cp -a node_modules lib/. build",
    "prelambda": "npm run build",
    "lambda": "cd build ; zip -r ../build.zip *",
    "connect": "npx postgraphile -c postgres://pixlcrypt_postgraphile:SuperSecret1234@localhost/$(whoami) --schema pixlcrypt",
    "connect_old": "npx postgraphile -c postgres://postgres@localhost/$(whoami) --schema pixlcrypt",
    "db": "make db --directory db",
    "docker:build": "docker build -t thepatrik/pixlcrypt .",
    "docker:push": "docker push 103824298747.dkr.ecr.eu-west-1.amazonaws.com/thepatrik/pixlcrypt:latest",
    "docker:run": "docker run --env-file ./.env thepatrik/pixlcrypt",
    "docker:tag": "docker tag thepatrik/pixlcrypt:latest 103824298747.dkr.ecr.eu-west-1.amazonaws.com/thepatrik/pixlcrypt:latest",
    "eslint": "./node_modules/.bin/eslint lib test",
    "eslint:fix": "./node_modules/.bin/eslint lib test --fix",
    "start": "node lib",
    "pretest": "make recreate-with-data --directory db",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --reporter spec --bail --exit",
    "validate": "npm run eslint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thepatrik/pixlcrypt.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thepatrik/pixlcrypt/issues"
  },
  "homepage": "https://github.com/thepatrik/pixlcrypt#readme",
  "dependencies": {
    "async": "^2.6.1",
    "aws-sdk": "^2.247.1",
    "aws-serverless-express": "^3.2.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-middleware": "^0.8.0",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-unless": "^0.5.0",
    "helmet": "^3.12.0",
    "postgraphile": "^4.0.0-beta.8",
    "restify-errors": "^6.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "fakefile": "0.0.9",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0"
  }
}
